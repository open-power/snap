      ah_brpar                          => a0h_brpar,
      ha_bwvalid                        => ha0_bwvalid,
      ha_bwtag                          => ha0_bwtag,
      ha_bwad                           => ha0_bwad,
      ha_bwdata                         => ha0_bwdata,
      ha_bwpar                          => ha0_bwpar,
