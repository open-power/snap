      hd1_sent_utag_sts                 => (OTHERS => '0'), --efes1024(2 downto 0),
      -- DMA port 1 Completion
      hd1_cpl_valid                     => '0',
      hd1_cpl_utag                      => (others => '0'), --efes1024(9 downto 0),
      hd1_cpl_type                      => (others => '0'), --efes1024(2 downto 0),
