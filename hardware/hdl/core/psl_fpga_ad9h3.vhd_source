      hd1_cpl_byte_count                => (others => '0'), --efes1024(9 downto 0),
      hd1_cpl_data                      => (others => '0')  --efes1024(1023 downto 0)
    );

    PROCESS (ha0_pclock)
    BEGIN
      IF(RISING_EDGE(ha0_pclock)) THEN
        user_image_q <= user_image_q;
      END IF;
    END PROCESS;

    gold_factory <= user_image_q;

