      hd0_cpl_byte_count                => hd0_cpl_byte_count,
      hd0_cpl_data                      => hd0_cpl_data,

      -- DMA port 1 Request
      d1h_dvalid                        => open,
      d1h_req_utag                      => open,
      d1h_req_itag                      => open,
      d1h_dtype                         => open,
      d1h_datomic_op                    => open,
      d1h_datomic_le                    => open,
